		<setting visible="false" id="CMI_Search1Channel" 					type="bool" label="Search 1Channel" 						default="true" />
		<setting visible="false" id="CMI_SearchPrimeWire" 				type="bool" label="(TV) - Search PrimeWire" 		default="true" />
