    <setting id="text-sub" type="text" label="(Sub)" default=" [COLOR blue](Sub)[/COLOR]" />
    <setting id="text-ova" type="text" label="OVA" default=" [COLOR red]OVA[/COLOR]" />
    <setting id="text-new" type="text" label="NEW" default=" [COLOR yellow]New[/COLOR]" />
    <setting id="text-movie" type="text" label="Movie" default=" [COLOR maroon]Movie[/COLOR]" />
